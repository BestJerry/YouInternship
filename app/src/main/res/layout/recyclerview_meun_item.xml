<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingStart="@dimen/EightDp"
    android:paddingEnd="@dimen/SixteenDp"
    android:paddingTop="@dimen/EightDp"
    android:paddingBottom="@dimen/EightDp"
    xmlns:tools="http://schemas.android.com/tools">

    <ImageView
        android:id="@+id/image_item_icon"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_centerVertical="true"/>


    <TextView
        android:layout_toEndOf="@id/image_item_icon"
        android:paddingStart="@dimen/ThirtyTwoDp"
        android:id="@+id/tv_item_describe"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:text="@string/my_resume"
        android:layout_centerVertical="true"/>

    <ImageView
        android:layout_alignParentEnd="true"
        android:id="@+id/image_item_arrow"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:src="@drawable/image_arrow"
        android:layout_centerVertical="true"/>


</RelativeLayout>